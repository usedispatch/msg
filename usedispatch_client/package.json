{
  "name": "@usedispatch/client",
  "version": "0.13.0",
  "description": "Client for Dispatch Protocol",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "clean": "rm -rf dist",
    "build": "yarn clean && parcel build",
    "format": "prettier --write \"src/*.ts\"",
    "idl": "mkdir -p lib/target/idl && mkdir -p lib/target/types && cp ../target/idl/* ./lib/target/idl/ && cp ../target/types/* ./lib/target/types/",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build && echo 'Please run the tests manually before publish'",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "source": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "module": {
      "optimize": true
    }
  },
  "keywords": [],
  "files": [
    "lib/**/*"
  ],
  "author": "Dispatch Protocol",
  "homepage": "https://usedispatch.net/",
  "license": "MIT or Apache 2.0",
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/core": "^2.7.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/bs58": "^4.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.11.12",
    "assert": "^2.0.0",
    "bs58": "^4.0.1",
    "jest": "^27.4.7",
    "parcel": "^2.7.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "rollup": "^2.79.0",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@bonfida/spl-name-service": "^0.1.30",
    "@metaplex-foundation/mpl-token-metadata": "^2.2.2",
    "@project-serum/anchor": "^0.24.2",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.50.1",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21"
  },
  "resolutions": {
    "typescript": "~4.7"
  },
  "packageManager": "yarn@3.2.3"
}
